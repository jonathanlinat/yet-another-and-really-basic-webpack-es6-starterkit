{
  "name": "yet-another-and-really-basic-webpack-es6-starterkit",
  "description": "A lightweight foundation for any Webpack 4 based frontend project, mainly inspired by WBKD's Webpack Frontend Starterkit.",
  "author": "Jonathan Linat (https://github.com/jonathanlinat)",
  "homepage": "https://github.com/jonathanlinat/yet-another-and-really-basic-webpack-es6-starterkit",
  "version": "1.2.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanlinat/yet-another-and-really-basic-webpack-es6-starterkit.git"
  },
  "main": "src/app.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --color",
    "build": "webpack --mode production"
  },
  "babel": {
    "presets": "babel-preset-env",
    "plugins": "transform-class-properties"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": "standard"
  },
  "keywords": [
    "npm",
    "html",
    "node",
    "sass",
    "babel",
    "eslint",
    "loaders",
    "server",
    "webpack",
    "javascript"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "node-watch": "^0.5.8",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
